<div id="wrapper">
<h1>Anniepix</h1>

	<h3><%= link_to "Back to Pictures", pictures_url %> | <%= link_to "Edit this Picture", edit_picture_path(@picture) %> | <%= link_to "Delete", @picture, :confirm => "Are you sure you want to delete this picture?", :method => :delete %></h3>

	<div class="show_image">
		<img src="<%= @picture[:url] %>">
		<p><strong>Title:</strong> <%= @picture[:title] %><br>
			<strong>Artist:</strong> <%= @picture[:artist] %></p>
	</div>
</div>